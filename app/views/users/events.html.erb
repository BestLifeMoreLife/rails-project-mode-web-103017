<h1>Your Events</h1>
<ul>
  <% @events.each do |e|%>
  <li><%= link_to e.name, event_path(e)%> | <%=e.category.name%> | <%=e.price%> | <%=e.date%></li>
</ul>

<%end%>

<h1>Events You're Attending</h1>
<ul>
  <% @events_attending.each do |e|%>
  <li><%= link_to e.name, event_path(e)%> | <%=e.category.name%> | <%=e.price%> | <%=e.date%></li>
</ul>

<%end%>
