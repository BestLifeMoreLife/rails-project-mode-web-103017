<h1> New User Form </h1>
<%= form_for @user do |f| %>
  <%= f.label :first_name %>
  <%= f.text_field :first_name %>
  <br></br>
  <%= f.label :last_name %>
  <%= f.text_field :last_name %>
  <br></br>
  <%= f.label :birthdate %>
  <%= f.date_select :birthdate, order:[:month, :day, :year] %>
  <br></br>
  <%= f.submit %>
<% end %>
